{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "dev": false
      }
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/.*",
      "dest": "src/main.jsx",
      "methods": ["GET", "POST", "PUT", "DELETE"],
      "headers": { "Access-Control-Allow-Origin": "*" }
    }
  ]
}
